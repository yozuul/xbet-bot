{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["tabs-manager.js", "timers.js", "bot.js"],
    "persistent": false
  },
  "permissions": ["storage", "declarativeContent", "activeTab", "tabs", "webNavigation", "https://*/*", "chrome://discards/"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon-16.png",
      "32": "/images/icon-32.png",
      "48": "/images/icon-48.png",
      "128": "/images/icon-128.png"
    }
  },
  "icons": {
    "16": "/images/icon-16.png",
    "32": "/images/icon-32.png",
    "48": "/images/icon-48.png",
    "128": "/images/icon-128.png"
  },
  "options_page": "options.html"
}
